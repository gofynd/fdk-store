    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "fdk-client-javascript": "github:gofynd/fdk-client-javascript#fyndx1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "buffer": "^6.0.3",
